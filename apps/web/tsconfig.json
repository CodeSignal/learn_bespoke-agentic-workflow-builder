{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "allowJs": false,
    "esModuleInterop": true,
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "@agentic/types": [
        "../../packages/types/src"
      ]
    },
    "declaration": false,
    "declarationMap": false
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}

